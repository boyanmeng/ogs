<?xml version="1.0" encoding="ISO-8859-1"?>
<OpenGeoSysProject>
    <mesh axially_symmetric="true">PCE_2D_axi.vtu</mesh>
    <geometry>PCE_2D_axi.gml</geometry>
    <processes>
        <process>
            <name>GW23</name>
            <type>THERMAL_TWOPHASE_COMPONENTIAL</type>
            <integration_order>2</integration_order>
            <process_variables>
                <gas_pressure> gas_pressure </gas_pressure>
                <overall_molar_fraction_air>overall_molar_fraction_air</overall_molar_fraction_air>
				<overall_molar_fraction_contaminant>overall_molar_fraction_contaminant</overall_molar_fraction_contaminant>
				<temperature>temperature</temperature>
            </process_variables>
            <jacobian_assembler>
                <type>CentralDifferences</type>
                <component_magnitudes>1e5 .9 1e-6 300</component_magnitudes>
                <relative_epsilons>1e-6 1e-6 1e-6 1e-6</relative_epsilons>
            </jacobian_assembler>
			<secondary_variables>
                <secondary_variable internal_name="saturation" output_name="saturation"/>
                <secondary_variable internal_name="capillary_pressure" output_name="capillary_pressure"/>
                <secondary_variable internal_name="pressure_wetting" output_name="pressure_wetting"/>
                <secondary_variable internal_name="liquid_molar_fraction_air" output_name="liquid_molar_fraction_air"/>
                <secondary_variable internal_name="liquid_molar_fraction_contaminant" output_name="liquid_molar_fraction_contaminant"/>
				<secondary_variable internal_name="gas_molar_fraction_water" output_name="gas_molar_fraction_water"/>
                <secondary_variable internal_name="gas_molar_fraction_contaminant" output_name="gas_molar_fraction_contaminant"/>
            </secondary_variables>
			<specific_body_force>0 -9.81</specific_body_force>
            <mass_lumping> true </mass_lumping>
	    </process>
    </processes>
    <media>
      <medium id="0">
         <phases>
            <phase>
               <type>AqueousLiquid</type>
			   <components>
                   <component>
				       <name>wasser</name>
                       <properties>
                           <property>
                               <name>density</name>
                               <type>Constant</type>
                               <value>1.0e3</value>
						   </property>
                       </properties>
				   </component>
				   <component>
				       <name>air</name>
                       <properties>
<!-- Daq=D_0exp(-E/T), PCE: 8e-10, air: 2e-9			   -->					   
                           <property>
                               <name>molecular_diffusion</name>
                               <type>Constant</type>
                               <value>2.0e-9</value>
                           </property>
                       </properties>
				   </component>
				   <component>
				       <name>contaminant</name>
                       <properties>
                           <property>
                               <name>molecular_diffusion</name>
                               <type>Constant</type>
                               <value>8.0e-10</value>
                           </property>
                       </properties>
				   </component>
			   </components>
               <properties>
                  <property>
                      <name>viscosity</name>
                      <type>Exponential</type>
                      <reference_value>1.0e-3</reference_value>
                      <exponent>
                          <variable_name>temperature</variable_name>
                          <reference_condition>293.15</reference_condition>
                          <factor>0.01333333</factor>
                      </exponent>
                  </property>
               </properties>
            </phase>
            <phase>
               <type>Gas</type>
			   <components>
                   <component>
                       <name>air</name>
                       <properties>
					       <property>
                              <name>density</name>
                              <type>IdealGasLaw</type>
                           </property>
                           <property>
                               <name>molar_mass</name>
                               <type>Constant</type>
                               <value>0.028964397787206768</value>
                           </property>
<!-- exponential with respect to 1/T						    -->
                           <!-- <property> -->
                               <!-- <name>Henry_constant</name> -->
                               <!-- <type>Constant</type> -->
                               <!-- <value>1</value> -->
                           <!-- </property> -->
						   <property>
                               <name>heat_capacity </name>
                               <type>Constant</type>
                              <value>1040</value>
                           </property>
                       </properties>
                   </component>
				   <component>
                       <name>wasser</name>
                       <properties>
					       <property>
                              <name>density</name>
                              <type>IdealGasLaw</type>
                           </property>
                           <property>
                               <name>molar_mass</name>
                               <type>Constant</type>
                               <value>0.18</value>
                           </property>
						   <property>
                              <name>molecular_diffusion</name>
<!-- Fuller's Law, power=1.75							   -->
                              <type>Constant</type>
                              <value>2.4e-5</value>
                           </property>
                           <property>
                               <name>heat_capacity </name>
                               <type>Constant</type>
                              <value>4187</value>
                           </property>						   
                       </properties>
                   </component>
				   <component>
                       <name>contaminant</name>
                       <properties>
						   <property>
                              <name>density</name>
                              <type>IdealGasLaw</type>
                           </property>
                           <property>
                               <name>molar_mass</name>
                               <type>Constant</type>
                               <value>0.16583</value>
                           </property>
<!-- exponential with respect to 1/T						    -->
                           <!-- <property> -->
                               <!-- <name>Henry_constant</name> -->
                               <!-- <type>Constant</type> -->
                               <!-- <value>1</value> -->
                           <!-- </property> -->
<!-- Fuller's Law, power=1.75							   -->
						   <property>
                              <name>molecular_diffusion</name>
                              <type>Constant</type>
                              <value>7e-6</value>
                           </property>
						   <property>
                              <name>heat_capacity </name>
                              <type>Constant</type>
                              <value>576</value>                <!-- Wikipedia PCE  -->   
                           </property>
                       </properties>
                   </component>
			   </components>
               <properties>
                  <property>
                     <name>density</name>
                     <type>IdealGasLaw</type>
                  </property>
<!-- dependence on T and composition	(square root increase with T, composition see Wilke's method)			   -->
                  <property>
                     <name>viscosity</name>
                     <type>Constant</type>
                     <value>1.8e-5</value>
                  </property>
                  <!-- <property> -->
                     <!-- <name>molar_mass</name> -->
                     <!-- <type>Constant</type> -->
                     <!-- <value>0.028964397787206768</value> -->
                  <!-- </property> -->
               </properties>
            </phase>
			<phase>
               <type>Solid</type>
               <properties>
                  <property>
                     <name>density</name>
                     <type>Constant</type>
                     <value>2650</value>
                  </property>
                  <property>
                      <name>heat_capacity </name>
                      <type>Constant</type>
                     <value>750</value>
                  </property>
               </properties>
            </phase>
         </phases>
         <properties>
            <property>
               <name>permeability</name>
               <type>Constant</type>
               <value>1.0e-10</value>
            </property>
<!-- currently Curve type is disabled, debug to find out the problem. -->
            <property>
               <name>capillary_pressure</name>
               <type>CapillaryPressureBrooksCorey</type>
               <residual_liquid_saturation>0.0</residual_liquid_saturation>
               <maximum_liquid_saturation>1.0</maximum_liquid_saturation>
               <lambda>2.0</lambda>
               <entry_pressure>5000</entry_pressure>
               <maximum_capillary_pressure>2e6</maximum_capillary_pressure>
            </property>
			<property>
               <name>saturation</name>
               <type>SaturationBrooksCorey</type>
               <residual_liquid_saturation>0.0</residual_liquid_saturation>
               <residual_gas_saturation>0.0</residual_gas_saturation>
               <lambda>2.0</lambda>
               <entry_pressure>5000</entry_pressure>
            </property>
            <property>
               <name>relative_permeability</name>
               <type>RelPermBrooksCorey</type>
               <residual_liquid_saturation>0.0</residual_liquid_saturation>
               <residual_gas_saturation>0.0</residual_gas_saturation>
               <lambda>2</lambda>
               <min_relative_permeability_liquid>0</min_relative_permeability_liquid>
               <min_relative_permeability_gas>1e-9</min_relative_permeability_gas>
            </property>
            <property>
               <name>porosity</name>
               <type>Constant</type>
               <value>0.3</value>
            </property>
<!-- square root or curve (2 types)			 -->
<!-- add to MPL property -->
			<property>
               <name>thermal_conductivity</name>
               <type>Curve</type>
               <curve>lambda_Sw</curve>
               <independent_variable>liquid_saturation</independent_variable>
            </property>	
         </properties>
      </medium>
    </media>	
    <parameters>
        <parameter>
            <name>pg_Dirichlet</name>
            <type>Constant</type>
            <value>101000</value>
        </parameter>
        <parameter>
            <name>Xa_0</name>
            <type>MeshNode</type>
            <field_name>Xa_ini</field_name>
        </parameter>
		<parameter>
            <name>Xc_0</name>
            <type>MeshNode</type>
            <field_name>Xc_ini</field_name>
        </parameter>
        <parameter>
            <name>pg_0</name>
            <type>MeshNode</type>
            <field_name>pg_ini</field_name>
        </parameter>
		<parameter>
            <name>T_0</name>
            <type>Constant</type>
            <value>283.15</value>
        </parameter>
        <parameter>
            <name>T_neumann</name>
            <type>Constant</type>
            <value>50</value>
        </parameter>
    </parameters>
    <time_loop>
        <processes>
            <process ref="GW23">
                <nonlinear_solver>basic_newton</nonlinear_solver>
                <convergence_criterion>
                    <type>PerComponentDeltaX</type>
                    <norm_type>NORM2</norm_type>
                    <reltols>1.e-6 1.e-5 1.e-5 1.e-5</reltols>
                </convergence_criterion>
                <time_discretization>
                    <type>CrankNicolson</type>
                    <theta>0.5</theta>
					<!-- <type>BackwardEuler</type> -->
                </time_discretization>
                <time_stepping>
                    <type>EvolutionaryPIDcontroller</type>
                    <t_initial> 0.0 </t_initial>
                    <t_end> 864000 </t_end>
                    <dt_guess> 100 </dt_guess>
                    <dt_min> 10 </dt_min>
                    <dt_max> 1e4 </dt_max>
                    <rel_dt_min> 0.01 </rel_dt_min>
                    <rel_dt_max> 1.5 </rel_dt_max>
                    <tol> 5 </tol>
                </time_stepping>
            </process>
        </processes>
        <output>
            <type>VTK</type>
            <prefix>PCE_thermal_partitioning_dt</prefix>
            <timesteps>
                <pair>
                    <repeat> 1 </repeat>
                    <each_steps> 1 </each_steps>
                </pair>
            </timesteps>
            <output_iteration_results>false</output_iteration_results>
            <variables>
                <variable> gas_pressure </variable>
                <variable> overall_molar_fraction_air </variable>
                <variable> overall_molar_fraction_contaminant </variable>
                <variable> temperature </variable>
                <variable> saturation </variable>
                <variable> capillary_pressure </variable>
                <variable> pressure_wetting </variable>
                <variable> liquid_molar_fraction_air </variable>
                <variable> liquid_molar_fraction_contaminant </variable>
                <variable> gas_molar_fraction_water </variable>
                <variable> gas_molar_fraction_contaminant </variable>
            </variables>
        </output>
    </time_loop>
    <process_variables>
        <process_variable>
            <name>gas_pressure</name>
            <components>1</components>
            <order>1</order>
            <initial_condition>pg_0</initial_condition>
            <boundary_conditions>
                <boundary_condition>
                    <geometrical_set>geometry</geometrical_set>
                    <geometry>top</geometry>
                    <type>Dirichlet</type>
                    <parameter>pg_Dirichlet</parameter>
                </boundary_condition>
            </boundary_conditions>
        </process_variable>
        <process_variable>
            <name>overall_molar_fraction_air</name>
            <components>1</components>
            <order>1</order>
            <initial_condition>Xa_0</initial_condition>
            <boundary_conditions>
            </boundary_conditions>
        </process_variable>
		<process_variable>
            <name>overall_molar_fraction_contaminant</name>
            <components>1</components>
            <order>1</order>
            <initial_condition>Xc_0</initial_condition>
            <boundary_conditions>
            </boundary_conditions>
        </process_variable>
		<process_variable>
            <name>temperature</name>
            <components>1</components>
            <order>1</order>
            <initial_condition>T_0</initial_condition>
            <boundary_conditions>
			    <boundary_condition>
                    <geometrical_set>geometry</geometrical_set>
                    <geometry>heat_source</geometry>
                    <type>Neumann</type>
                    <parameter>T_neumann</parameter>
                </boundary_condition>
            </boundary_conditions>
        </process_variable>
    </process_variables>
    <nonlinear_solvers>
        <nonlinear_solver>
            <name>     basic_newton </name>
            <type>     Newton       </type>
            <max_iter> 20          </max_iter>
            <linear_solver>general_linear_solver</linear_solver>
        </nonlinear_solver>
    </nonlinear_solvers>
    <linear_solvers>
        <linear_solver>
            <name>general_linear_solver</name>
            <lis>-i bicgstab -p ilu -tol 1e-18 -maxiter 10000</lis>
            <eigen>
                <solver_type>SparseLU</solver_type>
                <precon_type>ILUT</precon_type>
                <max_iteration_step>10000</max_iteration_step>
                <error_tolerance>1e-18</error_tolerance>
            </eigen>
            <petsc>
                <prefix>multiphase</prefix>
                <parameters>-gw_ksp_type cg -gw_pc_type bjacobi -gw_ksp_rtol 1e-16 -gw_ksp_max_it 10000</parameters>
            </petsc>
        </linear_solver>
    </linear_solvers>
